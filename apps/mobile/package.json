{
  "name": "@trainers/mobile",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "tsx ../../scripts/dev-with-ports.ts expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo export",
    "build:dev": "eas build --platform ios --profile development",
    "build:dev-device": "eas build --platform ios --profile development-device",
    "build:preview": "eas build --platform ios --profile preview",
    "_test": "jest",
    "_test:watch": "jest --watch",
    "lint": "eslint . --max-warnings 0 --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:ios": "expo prebuild --clean --platform ios",
    "prebuild:android": "expo prebuild --clean --platform android",
    "clean": "rm -rf .expo node_modules ios android"
  },
  "dependencies": {
    "@atproto/api": "^0.18.17",
    "@atproto/oauth-client-expo": "^0.0.8",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@pkmn/data": "^0.10.5",
    "@pkmn/dex": "^0.10.5",
    "@pkmn/sets": "^5.2.0",
    "@pkmn/sim": "^0.10.5",
    "@supabase/supabase-js": "^2.49.4",
    "@tamagui/config": "^1.144.3",
    "@tamagui/web": "^1.144.3",
    "@tanstack/react-query": "^5.90.19",
    "@trainers/supabase": "workspace:*",
    "@trainers/theme": "workspace:*",
    "@trainers/validators": "workspace:*",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.1",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tamagui": "^1.144.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/react-native": "^13.3.3",
    "@trainers/eslint-config": "workspace:*",
    "@trainers/jest-config": "workspace:*",
    "@trainers/prettier-config": "workspace:*",
    "@trainers/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "~19.1.10",
    "babel-plugin-react-compiler": "^1.0.0",
    "babel-preset-expo": "~54.0.10",
    "eslint": "catalog:",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "jest-expo": "^54.0.17",
    "typescript": "catalog:"
  },
  "prettier": "@trainers/prettier-config"
