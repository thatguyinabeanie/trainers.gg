{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "FORCE_REDIRECT_TO_HOME",
    "NEXT_PUBLIC_MAINTENANCE_MODE",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY",
    "NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_SITE_URL",
    "E2E_AUTH_BYPASS_SECRET",
    "SUPABASE_URL",
    "SUPABASE_PROJECT_REF",
    "SUPABASE_ACCESS_TOKEN",
    "SUPABASE_ANON_KEY",
    "SUPABASE_SERVICE_ROLE_KEY",
    "SUPABASE_JWT_SECRET",
    "SUPABASE_PUBLISHABLE_KEY",
    "SUPABASE_SECRET_KEY",
    "SUPABASE_POSTGRES_URL",
    "SUPABASE_POSTGRES_PRISMA_URL",
    "SUPABASE_POSTGRES_URL_NON_POOLING",
    "SUPABASE_POSTGRES_HOST",
    "SUPABASE_POSTGRES_USER",
    "SUPABASE_POSTGRES_PASSWORD",
    "SUPABASE_POSTGRES_DATABASE",
    "SUPABASE_AUTH_EXTERNAL_GITHUB_CLIENT_ID",
    "SUPABASE_AUTH_EXTERNAL_GITHUB_SECRET",
    "SUPABASE_AUTH_EXTERNAL_DISCORD_CLIENT_ID",
    "SUPABASE_AUTH_EXTERNAL_DISCORD_SECRET",
    "POSTGRES_URL",
    "POSTGRES_PRISMA_URL",
    "POSTGRES_URL_NON_POOLING",
    "POSTGRES_USER",
    "POSTGRES_HOST",
    "POSTGRES_PASSWORD",
    "POSTGRES_DATABASE",
    "ATPROTO_PRIVATE_KEY",
    "NGROK_STATIC_DOMAIN",
    "PDS_HANDLE_DOMAIN",
    "RESEND_API_KEY",
    "EXPO_PUBLIC_POSTHOG_KEY",
    "EXPO_PUBLIC_POSTHOG_HOST"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        ".expo/**",
        "src/generated/**"
      ],
      "env": [
        "NEXT_PUBLIC_POSTHOG_KEY",
        "NEXT_PUBLIC_POSTHOG_HOST",
        "EXPO_PUBLIC_POSTHOG_KEY",
        "EXPO_PUBLIC_POSTHOG_HOST"
      ]
    },
    "dev:setup": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev:setup", "dev:setup"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "format:check": {
      "outputs": []
    },
    "db:start": {
      "cache": false,
      "persistent": true
    },
    "db:stop": {
      "cache": false
    },
    "db:restart": {
      "cache": false,
      "persistent": true
    },
    "db:status": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:diff": {
      "cache": false
    },
    "db:lint": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:dump": {
      "cache": false
    },
    "prebuild": {
      "cache": false,
      "outputs": ["ios/**", "android/**"]
    },
    "prebuild:clean": {
      "cache": false,
      "outputs": ["ios/**", "android/**"]
    },
    "prebuild:ios": {
      "cache": false,
      "outputs": ["ios/**"]
    },
    "prebuild:android": {
      "cache": false,
      "outputs": ["android/**"]
    },
    "functions:serve": {
      "cache": false,
      "persistent": true
    },
    "generate-types": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"],
      "env": ["CI", "NODE_ENV"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "cache": false,
      "env": ["CI", "PLAYWRIGHT_BASE_URL"]
    }
  }
}
