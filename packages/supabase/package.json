{
  "name": "@trainers/supabase",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/clients/server.ts",
    "./client": "./src/clients/client.ts",
    "./mobile": "./src/clients/mobile.ts",
    "./hooks": "./src/hooks/index.ts",
    "./queries": "./src/queries/index.ts",
    "./mutations": "./src/mutations/index.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "setup": "./scripts/setup-local.sh",
    "dev:setup": "./scripts/setup-local.sh",
    "dev": "./scripts/dev.sh",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:restart": "supabase stop && supabase start",
    "db:status": "supabase status",
    "functions:serve": "supabase functions serve --no-verify-jwt --env-file .env",
    "generate-clients": "tsx scripts/generate-clients.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --max-warnings 0 --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "generate-types": "supabase gen types typescript --local 2>/dev/null > src/types.ts",
    "generate-seed-json": "tsx scripts/generate-seed-data/generate-user-data.ts",
    "generate-seeds": "tsx scripts/generate-seed-data/index.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset || (docker restart supabase_kong_supabase > /dev/null 2>&1 && sleep 2 && supabase status > /dev/null 2>&1)",
    "db:diff": "supabase db diff",
    "db:lint": "supabase db lint",
    "db:seed": "supabase db seed",
    "db:dump": "supabase db dump",
    "supabase": "supabase"
  },
  "dependencies": {
    "@supabase/supabase-js": "catalog:",
    "@trainers/utils": "workspace:*",
    "@trainers/validators": "workspace:*"
  },
  "peerDependencies": {
    "@supabase/ssr": "catalog:",
    "expo-secure-store": "~14.0.0",
    "next": "catalog:",
    "react": "catalog:",
    "react-native-url-polyfill": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@supabase/ssr": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-native-url-polyfill": {
      "optional": true
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/jest": "catalog:",
    "@jest/globals": "^30.2.0",
    "@trainers/eslint-config": "workspace:*",
    "@trainers/jest-config": "workspace:*",
    "@trainers/prettier-config": "workspace:*",
    "@trainers/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "jest": "catalog:",
    "jest-junit": "catalog:",
    "prettier": "catalog:",
    "supabase": "catalog:",
    "ts-jest": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@trainers/prettier-config"
}
