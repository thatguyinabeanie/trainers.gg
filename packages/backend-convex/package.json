{
  "name": "@trainers/backend-convex",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./convex/_generated/api": {
      "types": "./convex/_generated/api.d.ts",
      "default": "./convex/_generated/api.js"
    },
    "./convex/_generated/dataModel": {
      "types": "./convex/_generated/dataModel.d.ts",
      "default": "./convex/_generated/dataModel.js"
    },
    "./convex/permissionKeys": {
      "types": "./convex/permissionKeys.ts",
      "default": "./convex/permissionKeys.ts"
    }
  },
  "scripts": {
    "dev:convex": "convex dev",
    "deploy": "convex deploy",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"convex/**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"convex/**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .prettierignore"
  },
  "dependencies": {
    "@clerk/backend": "^2.0.0",
    "@clerk/nextjs": "^6.36.8",
    "convex": "^1.17.4",
    "svix": "^1.24.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@trainers/prettier-config": "workspace:*",
    "@trainers/typescript-config": "workspace:*",
    "typescript": "catalog:"
  },
  "prettier": "@trainers/prettier-config"
}
