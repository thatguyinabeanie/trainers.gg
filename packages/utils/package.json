{
  "name": "@trainers/utils",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --max-warnings 0 --cache",
    "typecheck": "tsc --noEmit",
