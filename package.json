{
  "name": "@thatguyinabeanie/trainers.gg",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@trainers/web",
    "dev:mobile": "turbo run dev --filter=@trainers/mobile",
    "dev:backend": "turbo run dev --filter=@trainers/supabase",
    "dev:web+backend": "turbo run dev --filter=@trainers/web --filter=@trainers/supabase",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@trainers/web",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "format": "turbo run format",
    "format:check": "turbo run format:check"
  },
  "devDependencies": {
    "@trainers/prettier-config": "workspace:*",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "turbo": "^2.7.5",
    "typescript": "catalog:"
  }
}
